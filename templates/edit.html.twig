{% extends 'base.html.twig' %}

{% block title %}Edit{% endblock %}

{% block body %}
    <h1>Edit</h1>

    {{ form_errors(form) }}

    {{ form_start(form) }}

    <div class="row">
        {{ form_row(form.name) }}
        {{ form_row(form.image) }}
        {{ form_row(form.submit) }}
    </div>

    {{ form_end(form) }}
{% endblock %}